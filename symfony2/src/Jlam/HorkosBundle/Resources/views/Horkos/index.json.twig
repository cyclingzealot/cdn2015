{
	"jurisdiction": {
		"name":					"{{summary.jurisdictionName}}",
		"wastedVotesPct":		"{{jurisdiction.wasted / jurisdiction.valid}}",
		"wastedVotes":			"{{jurisdiction.wasted}}",
		"participateRatePct":	"{{jurisdiction.all / jurisdiction.eligible}}"
	},
	"ridings": [
		{% for riding in ridings %}{
			"name":					"{{riding.name}}",
			"wastedVotesPct":		"{{riding.unrepresentedVotes / riding.validVotes * 100}}",
			"wastedVotes":			"{{riding.unrepresentedVotes}}",
			"participateRatePct":	"{{riding.participationRate * 100}}"
		}
		{% if not loop.last %},{% endif %}
		{% endfor %}
	]
}
